{
	"My Table": {
	  "prefix": "my table",
	  "body": [
		"\\begin{table}[h]",
		"\t\\centering",
		"\t\\caption{$1 \\strut}",
		"\t\\label{tab:$2}",
		"\t\\begin{tabular}{ccc}",
		"\t\t\\toprule",
		"\t\t$3 & $4 & $5 \\\\\\",
		"\t\t\\midrule",
		"\t\t & & \\\\\\",
		"\t\t & & \\\\\\",
		"\t\t\\bottomrule",
		"\t\t\\end{tabular}",
		"\\end{table}",
		"$0"
	  ],
	  "description": "Template for a nice table"
	},
	
	"Graphic Scaling": {"prefix": "scale", "body": "scale=0.$1"},

	"Graphic Width": {"prefix": "linewidth", "body": "width=\\linewidth"},
	
	"Center Graphic": {
		"prefix": "Centered Graphic", 
		"body": [
			"\\begin{center}",
        	"\t\\includegraphics[scale=0.$1]{images\/$2.png}",
			"\\end{center}",
			"\\vspace{-1cm}",
		]
	},

	"Color": { "prefix": "color", "body": "{\\color{$1}", "scope": "latex"},
	"Emphasis" : { "prefix": "emph", "body": "{\\color{o-Sun}", "scope": "latex"},
	"Answer" : { "prefix": "answ", "body": "{\\color{o-Sun}\\textbf{$1}}", "scope": "latex"},
	"Low Emphasis" : { "prefix": "lowcon", "body": "{\\color{darklc}", "scope": "latex"},
	"Positive Emphasis": { "prefix": "pos", "body": "{\\color{pos}", "scope": "latex"},
	"Negative Emphasis": { "prefix": "neg", "body": "{\\color{neg}", "scope": "latex"},
	"True": { "prefix": "true", "body": "{\\color{true}", "scope": "latex"},
	"False": { "prefix": "false", "body": "{\\color{false}", "scope": "latex"},
	"System": { "prefix": "sys", "body": "{\\color{sys}", "scope": "latex"},
	"Surr": { "prefix": "surr", "body": "{\\color{surr}", "scope": "latex"},


	"Cations": { "prefix": "cation", "body": "{\\color{pos}\\ch{$1^+}}"},
	"Hydrogen": { "prefix": "Hyd", "body": "{\\color{pos}\\ch{H^+}}"},
	"Anions": { "prefix": "anion", "body": "{\\color{neg}\\ch{$1^-}}"},

	"Inline alpha": { "prefix": "alpha", "body": "$\\alpha$"},
	"Inline beta": { "prefix": "beta", "body": "$\\beta$"},
	"Inline gamma": { "prefix": "gamma", "body": "$\\gamma$"},
	"Inline delta": { "prefix": "delta", "body": "$\\delta$"},
	"Inline sigma": { "prefix": "sigma", "body": "$\\sigma$"},
	"Inline pi": { "prefix": "pi", "body": "$\\pi$"},
	"Inline mu": { "prefix": "mu", "body": "$\\mu$"},
	"Inline rho": { "prefix": "rho", "body": "$\\rho$"},

	"O2": { "prefix": "O2", "body": "\\ch{O2}"},
	"CO2": { "prefix": "CO2", "body": "\\ch{CO2}"},
	"H2O": { "prefix": "H2O", "body": "\\ch{H2O}"},

	"UNIT": { "prefix": "sii", "body": "\\SI{$1}{$2}$0"},

	"Pka": { "prefix": "pka", "body": "\\(pK_a\\)"},
	"Pkb": { "prefix": "pkb", "body": "\\(pK_b\\)"},
	"Enthalpy": { "prefix": "bde", "body": "\\(\\Delta H^\\circ\\)"},
	"Entropy": { "prefix": "rsm", "body": "\\(\\Delta S^\\circ\\)"},
	"Gibbs Free Energy": { "prefix": "gfe", "body": "\\(\\Delta G^\\circ\\)"},
	"Equilibirum constant": { "prefix": "keq", "body": "\\(K_{\\text{eq}}\\)"},
	"Biomoleucar Reaction": { "prefix": "sn2", "body": "\\(S_N2\\) reactions"},
	"Uniomoleucar Reaction": { "prefix": "sn1", "body": "\\(S_N1\\) reactions"},
	"primary": { "prefix": "api", "body": "\\ang{1}"},
	"secondary": { "prefix": "asec", "body": "\\ang{2}"},
	"tertiary": { "prefix": "atert", "body": "\\ang{3}"},

	"Inline Subscript": {"prefix": "__t", "body": "\\(_{$1}\\)$0"},
	"Inline Superscript": {"prefix": "**t", "body": "\\(^{$1}\\)$0"},

	"Inline Hat": {"prefix": "hat", "body": "\\(\\hat{$1}\\)$0"},

	"Subsection": {
		"prefix": "Rubsection", 
		"body": [
			"\\subsection{$1}",
			"\\begin{itemize}",
			"\t\\item $0",
			"\\end{itemize}",
		]
	},

	"Subsubsection": {
		"prefix": "Wubsection", 
		"body": [
			"\\subsubsection{$1}",
			"\\begin{itemize}",
			"\t\\item $0",
			"\\end{itemize}",
		]
	},

	"New Chapter": {
		"prefix": "New Chapter", 
		"body": [
			"%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Chapter $1 %%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
			"%\\begingroup",
			"\\clearpage",
			"\\section{$3}\\phantomsection",
			"\\subsection{$4}",
			"\\begin{itemize}",
			"\t\\item $0",
			"\\end{itemize}",
			"%\\endgroup",
			"%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Chapter $2 %%%%%%%%%%%%%%%%%%%%%%%%%%%%%",
		]
	},

	"New Part": {
		"prefix": "Part",
		"body": [
			"\\clearpage",
			"\\fancyhead[L]{$1}",
			"%\\begingroup",
			"",
			"%\\endgroup",
		]
	}

  }